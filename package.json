{
  "name": "react-featureflow-client",
  "version": "1.2.0",
  "description": "React helpers for featureflow-client",
  "main": "dist/react.featureflow.js",
  "scripts": {
    "build-lib": "webpack src/index.js dist/react.featureflow.js",
    "build-min": "webpack -p src/index.js dist/react.featureflow.min.js",
    "build": "node ./scripts/build.js",
    "watch": "webpack --watch src/index.js dist/react.featureflow.js",
    "test": "karma start --single-run",
    "test:watch": "karma start --auto-watch --reporters=dots",
    "prepublish": "node ./scripts/build.js",
    "flow": "flow",
    "example": "webpack-dev-server --config ./example/webpack.config.js --content-base example/ --inline --port 8182"
  },
  "devDependencies": {
    "babel": "^6.23.0",
    "babel-core": "^6.23.1",
    "babel-loader": "^6.3.0",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.23.0",
    "chai": "3.5.0",
    "css-loader": "^0.26.1",
    "file-loader": "^0.10.0",
    "flow-bin": "^0.39.0",
    "gzip-size": "3.0.0",
    "html-webpack-plugin": "^2.28.0",
    "in-publish": "2.0.0",
    "karma": "0.13.22",
    "karma-chai": "0.1.0",
    "karma-chrome-launcher": "1.0.1",
    "karma-mocha": "1.0.1",
    "karma-mocha-reporter": "2.0.4",
    "karma-phantomjs-launcher": "0.2.1",
    "karma-phantomjs-shim": "1.1.2",
    "karma-sinon": "1.0.5",
    "karma-sourcemap-loader": "0.3.7",
    "karma-webpack": "2.0.2",
    "mocha": "2.5.3",
    "phantomjs": "1.9.18",
    "pretty-bytes": "3.0.1",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "readline-sync": "1.4.4",
    "semver-compare": "^1.0.0",
    "sinon": "1.17.4",
    "style-loader": "^0.13.1",
    "webpack": "2.2.1",
    "webpack-dev-server": "^2.3.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/featureflow/react-featureflow-client.git"
  },
  "keywords": [
    "react",
    "featureflow",
    "featureflow-client"
  ],
  "files": [
    "dist"
  ],
  "author": "Henry Young <henry.r.t.young@gmail.com>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/featureflow/react-featureflow-client/issues"
  },
  "homepage": "https://github.com/featureflow/react-featureflow-client#readme",
  "dependencies": {
    "react-children-utilities": "^0.1.6"
  },
  "peerDependencies": {
    "react": "^0.14.0 || ^15.0.0-0 || ^16.0.0-0",
    "featureflow-client": "^1.2.0"
  }
}
